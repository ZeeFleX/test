{
  "name": "service",
  "version": "1.0.0",
  "description": "TypeScript app",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "compile": "tsc",
    "dev": "node_modules/.bin/nodemon -e ts,json,js ./server/index.ts | pino-pretty",
    "dev:debug": "node_modules/.bin/nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "db:migrate:dev": "node_modules/.bin/sequelize db:migrate --env development",
    "db:migrate:prod": "node_modules/.bin/sequelize db:migrate --env production",
    "db:migrate:dev:undo": "node_modules/.bin/sequelize db:migrate:undo --env development",
    "db:migrate:dev:undo:all": "node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "db:migrate:prod:undo:": "node_modules/.bin/sequelize db:migrate:undo --env production",
    "db:migrate:prod:undo:all:": "node_modules/.bin/sequelize db:migrate:undo:all --env production"
  },
  "dependencies": {
    "@types/node": "^13.11.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.9.5",
    "mysql2": "^2.1.0",
    "pino": "^5.17.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/http-server": "^0.10.0",
    "@types/mocha": "^7.0.2",
    "@types/pino": "^5.17.0",
    "@types/sequelize": "^4.28.8",
    "@types/socket.io": "^2.1.4",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2",
    "pino-pretty": "^3.6.1",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
